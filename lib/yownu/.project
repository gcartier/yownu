        (<Jazz-File>       name: boot-processor     source: {File :context "src" "yownu" "boot-processor.jazz"})
        (<Jazz-File>       name: boot               source: {File :context "src" "yownu" "boot.jazz"}))
      (<Jazz-File>         name: y                  source: {File :context "src" "y.jazz"})
