(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: yownu           description-file: {File :context ".package"}
    (<*>                                         tag-reference: {File :context "yownu.project"})
    (<*>                                         tag-reference: {File :context "yownu-server.project"})
    (<*>                                         tag-reference: {File :context "yownu-worker.project"})
    (<Project-Directory>                         directory: {Directory :context "src"}
      (<Project-Directory>                       directory: {Directory :context "src" "yownu"}
        (<Jazz-File>       name: client          source: {File :context "src" "yownu" "client.jazz"})
        (<Jazz-File>       name: run             source: {File :context "src" "yownu" "run.jazz"})
        (<Jazz-File>       name: window          source: {File :context "src" "yownu" "window.jazz"})
        (<Jazz-File>       name: world           source: {File :context "src" "yownu" "world.jazz"})
        (<Jazz-File>       name: zone            source: {File :context "src" "yownu" "zone.jazz"})
        (<Jazz-File>       name: biome           source: {File :context "src" "yownu" "biome.jazz"})
        (<Jazz-File>       name: product         source: {File :context "src" "yownu" "product.jazz"})
        (<Jazz-File>       name: windows         source: {File :context "src" "yownu" "windows.jazz"})
        (<Jazz-File>       name: start           source: {File :context "src" "yownu" "start.jazz"})
        (<Jazz-File>       name: debugger        source: {File :context "src" "yownu" "debugger.jazz"})
        (<Jazz-File>       name: script          source: {File :context "src" "yownu" "script.jazz"})
        (<Jazz-File>       name: run-server      source: {File :context "src" "yownu" "run-server.jazz"})
        (<Jazz-File>       name: server          source: {File :context "src" "yownu" "server.jazz"})
        (<Jazz-File>       name: start-server    source: {File :context "src" "yownu" "start-server.jazz"})
        (<Jazz-File>       name: run-worker      source: {File :context "src" "yownu" "run-worker.jazz"})
        (<Jazz-File>       name: worker          source: {File :context "src" "yownu" "worker.jazz"})
        (<Jazz-File>       name: worker-settings source: {File :context "src" "yownu" "worker-settings.jazz"}))))))
