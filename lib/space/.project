;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: space   description-file: {File :context ".package"}
    (<*>                                 tag-reference: {File :context "space.project"})
    (<Project-Directory>                 directory: {Directory :context "src"}
      (<Project-Directory>               directory: {Directory :context "src" "space"}
        (<Jazz-File>       name: client  source: {File :context "src" "space" "client.jazz"})
        (<Jazz-File>       name: run     source: {File :context "src" "space" "run.jazz"})
        (<Jazz-File>       name: window  source: {File :context "src" "space" "window.jazz"})
        (<Jazz-File>       name: world   source: {File :context "src" "space" "world.jazz"})
        (<Jazz-File>       name: zone    source: {File :context "src" "space" "zone.jazz"})
        (<Jazz-File>       name: biome   source: {File :context "src" "space" "biome.jazz"})
        (<Jazz-File>       name: product source: {File :context "src" "space" "product.jazz"})
        (<Jazz-File>       name: windows source: {File :context "src" "space" "windows.jazz"})
        (<Jazz-File>       name: start   source: {File :context "src" "space" "start.jazz"}))))))
